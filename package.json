{
  "name": "starbucks-dynamic-website",
  "version": "1.0.0",
  "private": true,
  "description": "Dynamic Starbucks website built with React + Tailwind CSS for Frontend Hackathon",
  "author": "Aadi",
  "scripts": {
    "dev": "vite",
    "build": "NODE_OPTIONS='--max-old-space-size=2048' vite build",
    "build:prod": "NODE_OPTIONS='--max-old-space-size=1024' vite build",
    "preview": "vite preview --config vite.config.prod.js",
    "start": "npm run build:prod && npm run preview",
    "serve": "node server.js",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "setup:aws": "node scripts/setup-aws-tables.js"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch": "^3.908.0",
    "@aws-sdk/client-dynamodb": "^3.908.0",
    "@aws-sdk/client-iam": "^3.908.0",
    "@aws-sdk/lib-dynamodb": "^3.908.0",
    "aws-sdk": "^2.1562.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "framer-motion": "^11.0.5",
    "lucide-react": "^0.344.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intersection-observer": "^9.8.1",
    "react-router-dom": "^6.22.0",
    "recharts": "^3.2.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "vite": "^5.1.4"
  },
  "type": "module"
}
